{
  "name": "collaborative-canvas",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.ts",
    "client": "vite client",
    "build": "tsc && vite build",
    "start": "node dist/server.js",
    "vercel-build": "tsc && vite build client"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.8.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.2",
    "@types/uuid": "^9.0.2",
    "@vercel/node": "^2.0.0",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  },
  "engines": {
    "node": ">=18"
  }
}
