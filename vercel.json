{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/client",
  "devCommand": "npm run dev",
  "cleanUrls": true,
  "rewrites": [
    { 
      "source": "/socket.io/:match*",
      "destination": "/api/socketio/:match*"
    },
    {
      "source": "/api/:match*",
      "destination": "/api/:match*"
    }
  ]
}